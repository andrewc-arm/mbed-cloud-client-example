{
    "macros": [
        "PAL_BOARD_SPECIFIC_CONFIG=\"pal_board_specific.h\"",
        "ARM_UC_USE_PAL_BLOCKDEVICE=1",
        "CELLULAR_DEVICE=SAMSUNG_S5JS100_PPP_RIL", 
        "MDMRXD=NC", 
        "MDMTXD=NC",
        "MDMRTS=NC", 
        "MDMCTS=NC", 
        "MBEDTLS_TEST_NULL_ENTROPY", 
        "MBEDTLS_NO_DEFAULT_ENTROPY_SOURCES",
        "ARM_UC_FEATURE_PAL_FLASHIAP=1"
    ],
    "target_overrides": {
        "*": {
            "nanostack-hal.event_loop_thread_stack_size" : 8192,
            "platform.stdio-baud-rate"          : 115200,
            "platform.stdio-convert-newlines"   : true,
            "platform.default-serial-baud-rate" : 115200,
            "lwip.ipv4-enabled"                 : true,
            "lwip.ipv6-enabled"                 : false,
            "lwip.ppp-enabled"                  : false,
            "lwip.tcp-enabled"                  : true,
            "mbed-client.event-loop-size"       : 2048,
            "mbed-client.sn-coap-duplication-max-msgs-count": 1,
            "mbed-trace.enable"                 : 1,
            "target.features_add"               : ["STORAGE"],
            "target.components_add"             : ["FLASHIAP"]
        },
        "S5JS100": {
            "lwip.tcpip-thread-stacksize"       : 2048,
            "lwip.mem-size"                     : 25600,
            "target.network-default-interface-type" : "CELLULAR",
            "cellular.debug-at"                 : false,
            "cellular.use-apn-lookup"           : false,
            "nsapi.dns-response-wait-time"      : 30000,
            "client_app.pal_dtls_peer_min_timeout" : "20000",
            "client_app.partition_mode"         : 0,
            "client_app.primary_partition_size" : "(1024*1024)",
            "client_app.sotp-section-1-address" : "0x40EF5000",
            "client_app.sotp-section-1-size"    : "0x1000",
            "client_app.sotp-section-2-address" : "0x40EF6000",
            "client_app.sotp-section-2-size"    : "0x1000",
            "nsapi.default-cellular-plmn"       : 0,
            "nsapi.default-cellular-sim-pin"    : 0,
            "nsapi.default-cellular-apn"        : "\"kt.pilot.ip.ktnbiot\"",
            "nsapi.default-cellular-username"   : 0,
            "nsapi.default-cellular-password"   : 0
        }
    },
    "config": {
        "developer-mode": {
            "help": "Enable Developer mode to skip Factory enrollment",
            "value": 1
        },
        "trace-level": {
            "help": "Options are TRACE_LEVEL_ERROR,TRACE_LEVEL_WARN,TRACE_LEVEL_INFO,TRACE_LEVEL_DEBUG",
            "macro_name": "MBED_TRACE_MAX_LEVEL",
            "value": "TRACE_LEVEL_DEBUG"
        }
    }
}
